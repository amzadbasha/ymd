Liferay.Util.portletTitleEdit=function(){};AUI().use("aui-io-request","aui-overlay-context-panel",function(b){var c=b.one("#cpContextPanelTemplate");var e=b.one("#cpPortletTitleHelpIcon");if(c&&e){var f=c.attr("data-portlet-id");var h=(c.attr("data-visible-panel")=="true");var g={};var a="show-portlet-description-"+f;if(themeDisplay.isImpersonated()){g.doAsUserId=themeDisplay.getDoAsUserIdEncoded()}g[a]=false;c.show();var d=new b.OverlayContextPanel({align:{points:["tl","bl"]},bodyContent:c,on:{hide:function(){b.io.request(themeDisplay.getPathMain()+"/portal/session_click",{data:g})}},trigger:e,visible:false,width:450}).render();if(h){d.show()}}});